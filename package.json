    "node-fetch": "^2.6.8",
    "node-text-from-image": "^1.0.1",
    "pm2": "^5.2.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-friendly-formatter": "^4.0.1",
